{
    "name": "@dsqr/hoot",
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "build": "bun run scripts/build.ts"
    },
    "sideEffects": false,
    "devDependencies": {
        "bun-types": "latest",
        "typescript": "5.8.3"
    },
    "exports": {
